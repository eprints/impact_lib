<!-- Help Text -->	

	<!-- EPRINT -->

	<epp:phrase id="eprint_fieldhelp_imp_summary" type="XHTML">
      		<p>Please summarise the research that is underpinning your knowledge exchange/external engagement. (Max 100 words)</p>
  	</epp:phrase>
	<epp:phrase id="eprint_fieldhelp_imp_title" type="XHTML">
      		<p>The title of the research that is underpinning your knowledge exchange/external engagement</p>
  	</epp:phrase>
  	<epp:phrase id="eprint_fieldhelp_creator_impact" type="XHTML">
	      	<p>List the principal GU academic(s) involved (e.g. the principal applicant(s) - if this is a funded KE activity) who are within the same school/institute/unit.</p>
  	</epp:phrase>
	<epp:phrase id="eprint_fieldhelp_imp_evidence_link" type="XHTML">
	      		<p>THIS IS FOR DOCUMENTATION ALREADY ONLINE.</p>
		  	<p>For example, your documentation could be a link to another record in Enlighten:<br />
		  	<strong>http://eprints.gla.ac.uk/12345</strong></p>
		  	<p>Or it could be an article or document available from an external website:<br />
		  	<strong>http://some-newspaper/my-evidence</strong></p>
	      		<p>Or you may want to list the links to a project website, blog or social media site relevant to your KE and potential impact:<br />
		  	<strong>http://my-project-website-or-blog.com</strong></p>
	</epp:phrase>
		
	<epp:phrase id="eprint_fieldhelp_imp_engagement" type="XHTML">
      		<p></p>
    	</epp:phrase>
<!--
	<epp:phrase id="eprint_fieldhelp_imp_additional_funding" type="XHTML">
